@model List<ResultProductImageViewModel>
<div class="single-post-image mb-5">
    <div class="owl-blog owl-carousel owl-theme">
        @if (Model.Count > 0)
        {

            @foreach (var item in Model)
            {
                <div class="item">
                    <div class="card">
                        <img src="@item.ImageUrl" style="height:500px" class="img-fluid radius-image" alt="image">
                    </div>
                </div>
            }
        }
        else
        {
            <div class="item">
                <div class="card">
                    <img src="~/cityimages/dresden.jpg" class="img-fluid radius-image" alt="image">
                </div>
            </div>
        }
    </div>
</div>